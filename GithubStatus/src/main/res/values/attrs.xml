<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="autoScaleTextViewStyle" format="reference" />
    <declare-styleable name="AutoScaleTextView">
        <attr name="scaleFactor" format="fraction" />
        <attr name="minTextSize" format="dimension" />
        <attr name="maxTextSize" format="dimension" />
    </declare-styleable>

    <attr name="fontTextViewStyle" format="reference" />
    <declare-styleable name="FontTextView">
        <attr name="fontName" format="string"/>
    </declare-styleable>

    <attr name="sliceViewStyle" format="reference" />
    <declare-styleable name="SliceView">
        <attr name="sliceHeight" format="dimension" />
        <attr name="sliceOffset" format="dimension" />
    </declare-styleable>

</resources>